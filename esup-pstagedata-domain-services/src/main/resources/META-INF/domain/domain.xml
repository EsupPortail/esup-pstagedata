	</bean>
	<bean id="typeStructureDomainService"
		class="org.esupportail.pstagedata.domain.TypeStructureDomainServiceImpl">
		<property name="typeStructureDaoService" ref="typeStructureDaoService" />
	</bean>
	<bean id="uniteDureeDomainService"
		class="org.esupportail.pstagedata.domain.UniteDureeDomainServiceImpl">
		<property name="uniteDureeDaoService" ref="uniteDureeDaoService" />
	</bean>

	<!-- Convention -->

	<bean id="assuranceDomainService"
		class="org.esupportail.pstagedata.domain.AssuranceDomainServiceImpl">
		<property name="assuranceDaoService" ref="assuranceDaoService" />
	</bean>
	<bean id="caisseRegimeDomainService"
		class="org.esupportail.pstagedata.domain.CaisseRegimeDomainServiceImpl">
		<property name="caisseRegimeDaoService" ref="caisseRegimeDaoService" />
	</bean>
	<bean id="conventionDomainService"
		class="org.esupportail.pstagedata.domain.ConventionDomainServiceImpl">
		<property name="conventionDaoService" ref="conventionDaoService" />
		<property name="ufrDaoService" ref="ufrDaoService" />
		<property name="etapeDaoService" ref="etapeDaoService" />
	</bean>
	<bean id="enseignantDomainService"
		class="org.esupportail.pstagedata.domain.EnseignantDomainServiceImpl">
		<property name="enseignantDaoService" ref="enseignantDaoService" />
	</bean>
	<bean id="etapeDomainService"
		class="org.esupportail.pstagedata.domain.EtapeDomainServiceImpl">
		<property name="etapeDaoService" ref="etapeDaoService" />
	</bean>
	<bean id="etudiantDomainService"
		class="org.esupportail.pstagedata.domain.EtudiantDomainServiceImpl">
