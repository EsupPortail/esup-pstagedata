<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="CaisseRegime">
	<typeAlias alias="ClassCaisseRegime" type="org.esupportail.pstagedata.domain.beans.CaisseRegime"/>
	<resultMap id="CaisseRegime"
		class="ClassCaisseRegime">
		<result property="code" column="codeCaisse" />
		<result property="libelle" column="libelleCaisse" />
		<result property="infoCaisse" column="infoCaisse" />
		<result property="temEnServ" column="temEnServCaisse" />
	</resultMap>

	<statement id="getCaisseRegimes" resultMap="CaisseRegime">
		SELECT * FROM CaisseRegime
		WHERE temEnServCaisse = 'O'
	</statement>
	
	<statement id="getCaisseRegimeFromId" resultMap="CaisseRegime">
		SELECT * FROM CaisseRegime
		WHERE codeCaisse = #code#
		LIMIT 0, 1
	</statement>

</sqlMap>