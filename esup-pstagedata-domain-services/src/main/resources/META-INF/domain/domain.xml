	</bean>
	<bean id="langueConventionDomainService"
		class="org.esupportail.pstagedata.domain.LangueConventionDomainServiceImpl">
		<property name="langueConventionDaoService" ref="langueConventionDaoService" />
	</bean>
	<bean id="modeValidationStageDomainService"
		class="org.esupportail.pstagedata.domain.ModeValidationStageDomainServiceImpl">
		<property name="modeValidationStageDaoService" ref="modeValidationStageDaoService" />
	</bean>
	<bean id="modeVersGratificationDomainService"
		class="org.esupportail.pstagedata.domain.ModeVersGratificationDomainServiceImpl">
		<property name="modeVersGratificationDaoService" ref="modeVersGratificationDaoService" />
	</bean>
	<bean id="natureTravailDomainService"
		class="org.esupportail.pstagedata.domain.NatureTravailDomainServiceImpl">
		<property name="natureTravailDaoService" ref="natureTravailDaoService" />
	</bean>
	<bean id="themeDomainService"
		class="org.esupportail.pstagedata.domain.ThemeDomainServiceImpl">
		<property name="themeDaoService" ref="themeDaoService" />
	</bean>
	<bean id="typeConventionDomainService"
		class="org.esupportail.pstagedata.domain.TypeConventionDomainServiceImpl">
		<property name="typeConventionDaoService" ref="typeConventionDaoService" />
	</bean>
	<bean id="ufrDomainService" class="org.esupportail.pstagedata.domain.UfrDomainServiceImpl">
		<property name="ufrDaoService" ref="ufrDaoService" />
	</bean>
	<bean id="uniteGratificationDomainService"
		class="org.esupportail.pstagedata.domain.UniteGratificationDomainServiceImpl">
		<property name="uniteGratificationDaoService" ref="uniteGratificationDaoService" />
	</bean>
	<bean id="origineStageDomainService"
		class="org.esupportail.pstagedata.domain.OrigineStageDomainServiceImpl">
		<property name="origineStageDaoService" ref="origineStageDaoService" />
	</bean>

	<bean id="ticketStructureDomainService"
		class="org.esupportail.pstagedata.domain.TicketStructureDomainServiceImpl">
		<property name="ticketStructureDaoService" ref="ticketStructureDaoService" />
	</bean>

	<!-- ############################################## Debut statistiques ################################################################################################# -->
	<bean id="critereDomainService"
		class="org.esupportail.pstagedata.domain.CritereDomainServiceImpl">
		<property name="critereDaoService" ref="critereDaoService" />
		<property name="critereDTOManager" ref="critereDTOManager" />
	</bean>
	<bean id="critereDTOManager" class="org.esupportail.pstagedata.domain.CritereDTOManager" />
	<bean id="critereDTOFactory" factory-bean="critereDTOManager"
		factory-method="createCritereDTO" />

	<bean id="statisticsDtoManager"
		class="org.esupportail.pstagedata.domain.StatisticsDtoManagerImpl" />
	<bean id="statisticsDtoFactory" factory-bean="statisticsDtoManager"
		factory-method="createStatisticItemDTO" />


	<bean id="abstractOffreStatsDomaine" abstract="true"
		init-method="initOffreStatsDomaine">
		<property name="statisticsDtoManager" ref="statisticsDtoManager" />
	</bean>

	<!-- createur de **StatsdomaineService pour les offres -->
	<bean id="offreStatsDomaineManager"
		class="org.esupportail.pstagedata.domain.OffreStatsDomaineManagerImpl"
		lazy-init="true">
		<property name="offreStatsDomaineServices">
			<map>
				<entry key="${offre.etat.sans}">
					<bean
						class="org.esupportail.pstagedata.domain.OffreStatsDomaineServiceImpl"
						parent="abstractOffreStatsDomaine">
						<property name="offreStatsDaoService" ref="offreStatsDaoService" />
					</bean>
				</entry>
			</map>
		</property>
	</bean>

	<bean id="offreStatsDomaineService" factory-bean="offreStatsDomaineManager"
		factory-method="create">
		<constructor-arg value="${offre.etat.sans}" />
	</bean>


	<!-- createur de **StatsdomaineService pour les conventions -->

	<bean id="conventionStatsDomaineManager"
		class="org.esupportail.pstagedata.domain.ConventionStatsDomaineManagerImpl"
		lazy-init="true">
		<property name="cvtsDomaineServices">
			<map>
				<entry key="${convention.pour.cdg}">
					<bean
						class="org.esupportail.pstagedata.domain.ConventionStatsDomaineServiceImpl">
						<property name="conventionStatsDaoService" ref="centreGestionConventionStatsDaoService" />
						<property name="statisticsDtoManager" ref="statisticsDtoManager" />
					</bean>
				</entry>
				<entry key="${convention.pour.toute.univ}">
					<bean
						class="org.esupportail.pstagedata.domain.ConventionStatsDomaineServiceImpl">
						<property name="conventionStatsDaoService" ref="globalConventionStatsDaoService" />
						<property name="statisticsDtoManager" ref="statisticsDtoManager" />
					</bean>
				</entry>
			</map>
		</property>
	</bean>

	<bean id="centreGestionCvtStatsDomaineService" factory-bean="conventionStatsDomaineManager"
		factory-method="creer">
		<constructor-arg value="${convention.pour.cdg}" />
	</bean>
	<bean id="globalConventionStatsDomaineService" factory-bean="conventionStatsDomaineManager"
		factory-method="creer">
		<constructor-arg value="${convention.pour.toute.univ}" />
	</bean>

	<!-- ############################################## Fin statistiques ############################################## -->


	<bean id="domainService" class="org.esupportail.pstagedata.domain.DomainServiceImpl" />

</beans>
